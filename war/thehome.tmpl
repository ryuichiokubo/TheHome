<html>
<head>
<title>TheHome</title>
</head>

<body>
	<div id=contents></div>

<script src="/jquery-2.0.3.js" type="text/javascript"></script>
<script>
$( document ).ready(function() {
	$("#contents").append("<p>${feedTitle}</p>");
	<#if articles??>
		<#list articles as article>
			var hash = "${article.hash}";
			var title = "${article.title}";
			localStorage.setItem(hash, title);
			$("#contents").append("<a href=${article.link}>${article.title}</a>");
			$("#contents").append("<br />");
			$("#contents").append("${article.summary}");
			$("#contents").append("<br /><br />");
		</#list>
	</#if>
});
</script>
</body>
</html>
